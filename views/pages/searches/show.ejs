<!DOCTYPE html>
<html>

<head>
  <%- include('./layouts/head') %>
</head>

<body>
  <h1>Books by </h1>
  <p><a href="/">Home</a></p>

  <ul>
    <% searchResults.forEach(item => { %>
    <li>
      <img src=<%= item.image %>>
     <h3> <%= item.title %> </h3>
      <i><%= item.author %></i>
      <p class="description"><%= item.description %></p>
    </li>
    <% }); %>
    
  </ul>
  
</body>

</html>