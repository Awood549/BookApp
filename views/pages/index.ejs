<!DOCTYPE html>
<html lang="en">
  <head>
      <%- include('./layouts/head') %>
  </head>
  <body>
    <%- include('../layout/header') %>
    <main>
      <h4>Saved Books</h4>
      <p><%= books.length %> Books in the Collection</p>
      <ul id="book-list">
        <% books.forEach(book => %>
        <li class="book-items" data-book-id="<%=
        book.id %>">
        <%- include('book/detai', {book: book}) %>
        <p class="detail-button"><a href="/books/<%=
        book.id %>">View Deatils</a></p>
        </li>
        <% ) %>
      </ul>
     
    </main>
  </body>
</html>